{
  "name": "freeagentics",
  "version": "0.1.0",
  "description": "FreeAgentics - A multi-agent AI platform for emergent cognitive networks",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings=0",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ci": "CI=true playwright test --reporter=list",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:unit && npm run test:e2e",
    "coverage:view": "open coverage/lcov-report/index.html",
    "coverage:upload": "codecov",
    "coverage:report": "node scripts/coverage-report.js",
    "coverage:full": "node scripts/coverage-report.js full",
    "coverage:badge": "node scripts/coverage-report.js",
    "clean": "rm -rf .next out coverage",
    "quality": "npm run type-check && npm run lint:strict && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "quality:full": "npm run quality && npm run test:coverage",
    "precommit": "lint-staged",
    "validate": "npm run quality:full && npm run build",
    "preproduction": "npm run clean && npm run quality:full && npm run build && npm run test:e2e",
    "preproduction:fast": "npm run quality && npm run build && npm run test:unit",
    "analyze": "ANALYZE=true npm run build",
    "find-deadcode": "ts-prune --skip node_modules",
    "check-deps": "npx depcheck",
    "check-updates": "npx npm-check-updates",
    "update-deps": "npx npm-check-updates -u && npm install",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "python:lint": "cd src && black . --check && isort . --check-only && flake8 . --max-line-length=88 --extend-ignore=E203,W503",
    "python:format": "cd src && black . && isort .",
    "python:type": "cd src && mypy . --ignore-missing-imports",
    "python:test": "cd src && pytest tests/ -v --cov=. --cov-report=html --cov-report=term",
    "python:quality": "npm run python:lint && npm run python:type",
    "python:quality:fix": "npm run python:format",
    "backend:quality": "npm run python:quality && npm run python:test",
    "all:quality": "npm run quality:full && npm run backend:quality",
    "all:format": "npm run format && npm run python:format",
    "freeagentics": "node scripts/freeagentics-cli.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-spring/web": "^10.0.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-table": "^8.11.2",
    "@types/d3": "^7.4.3",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/react-window": "^1.8.8",
    "@types/three": "^0.160.0",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "d3": "^7.9.0",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.0.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.19.1",
    "input-otp": "^1.4.2",
    "jszip": "^3.10.1",
    "katex": "^0.16.22",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.309.0",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "react-force-graph-2d": "^1.27.1",
    "react-hook-form": "^7.48.2",
    "react-redux": "^9.2.0",
    "react-resizable-panels": "^1.0.10",
    "react-sparklines": "^1.7.0",
    "react-use-measure": "^2.1.7",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.15.4",
    "socket.io-client": "^4.8.1",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.160.1",
    "vaul": "^0.8.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@playwright/test": "^1.53.1",
    "@size-limit/preset-app": "^11.0.1",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/katex": "^0.16.7",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "autoprefixer": "^10.4.16",
    "codecov": "^3.8.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "size-limit": "^8.2.6",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
